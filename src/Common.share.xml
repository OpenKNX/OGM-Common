                            <choose ParamRefId="%AID%_UP-%TT%00159_R-%TT%0015901">
                              <when test="0">
                                <!-- Summertime active? -->
                                <ComObjectRefRef RefId="%AID%_O-%TT%00010_R-%TT%0001001" />
